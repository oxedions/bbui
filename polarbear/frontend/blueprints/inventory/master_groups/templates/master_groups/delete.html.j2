<h1 class="title">Delete Master Group</h1>
  <h2 class="subtitle">
    Add a new Master Group to cluster.<br>
  </h2>

<form action="/inventory/master_groups/delete.html" method="POST">

{% set mg_list = [] %}
{% for mg in master_groups_data %}{% set mg_list = mg_list.append(mg) %}{% endfor %}
{{ field_select("Select master group to delete:", "master_group_name", mg_list, mg_list[0]) }}

  <div class="field is-grouped">
    <div class="control">
      <button class="button is-link">Submit</button>
    </div>
  </div>

</form>
